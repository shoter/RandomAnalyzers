    <PackageTags>RandomAnalyzers.RequiredMember, analyzers</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <RootNamespace>RandomAnalyzers.RequiredMember</RootNamespace>
    <PackageReleaseNotes>0.1.0:
Test upload</PackageReleaseNotes>
    <RepositoryType>Github</RepositoryType>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
    <Version>0.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RandomAnalyzers.RequiredMember.Analyzer" Version="0.1.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
