<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>RandomAnalyzers.RequiredMember.Lib</AssemblyName>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>RandomAnalyzers.RequiredMember</PackageId>
    <Authors>Shoter</Authors>
    <Product>RandomAnalyzers.RequiredMember</Product>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <Company>Shoter</Company>
